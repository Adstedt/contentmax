{
  "name": "contentmax",
  "version": "1.0.0",
  "description": "AI-powered content generation platform for scalable content creation",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:sitemap": "tsx scripts/test-sitemap.ts",
    "test:feed": "tsx scripts/test-feed-taxonomy.ts",
    "test:feed:url": "tsx scripts/test-feed-url.ts",
    "import:feed": "tsx scripts/import-to-db.ts",
    "db:inspect": "tsx scripts/inspect-database.ts",
    "test:migration": "jest tests/migrations/009_node_centric_model.test.ts",
    "ci": "npm run lint && npm run type-check && npm run test:ci",
    "prepare": "husky",
    "local:start": "powershell -ExecutionPolicy Bypass -File ./scripts/start-local-dev.ps1",
    "local:start:bash": "bash ./scripts/start-local-dev.sh",
    "local:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:migration:new": "supabase migration new",
    "db:migration:up": "supabase migration up",
    "db:types": "supabase gen types typescript --local > types/database.types.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adstedt/contentmax.git"
  },
  "keywords": [
    "ai",
    "content-generation",
    "nextjs",
    "typescript",
    "supabase"
  ],
  "author": "ContentMax Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Adstedt/contentmax/issues"
  },
  "homepage": "https://github.com/Adstedt/contentmax#readme",
